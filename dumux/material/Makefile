install-materialHEADERS: $(material_HEADERS)
	@$(NORMAL_INSTALL)
	test -z "$(materialdir)" || $(MKDIR_P) "$(DESTDIR)$(materialdir)"
	@list='$(material_HEADERS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(materialHEADERS_INSTALL) '$$d$$p' '$(DESTDIR)$(materialdir)/$$f'"; \
	  $(materialHEADERS_INSTALL) "$$d$$p" "$(DESTDIR)$(materialdir)/$$f"; \
	done

