[TimeLoop]
TStart = 0.5e-6
DtInitial = 0.1e-6 # [s]
#MaxTimeStepSize = 1e-6
TEnd = 1e-6 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 50 50

[Problem]
Name = test_angeli # name passed to the output routines
EnableGravity = false
PrintL2Error = false
EnableInertiaTerms = true
# PrintMatrix = true


[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.1

[Assembly]
NumericDifference.BaseEpsilon = 1e-8

[Newton]
MaxSteps = 10
MaxRelativeShift = 1e-8

[Vtk]
WriteFaceData = true

[Flux]
UpwindWeight = 0.5

[FreeFlow]
EnableUnsymmetrizedVelocityGradient = false
UseOldTransportingVelocity = false
