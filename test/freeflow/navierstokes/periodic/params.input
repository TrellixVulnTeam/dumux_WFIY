[Grid]
File = periodic.dgf

[Problem]
Name = test_kovasznay # name passed to the output routines
EnableGravity = true
PrintL2Error = false
EnableInertiaTerms = false
PrintMatrix = true

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.025

[ Newton ]
MaxSteps = 10
MaxRelativeShift = 1e-5

[Vtk]
WriteFaceData = false

[Flux]
UpwindWeight = 0.5
