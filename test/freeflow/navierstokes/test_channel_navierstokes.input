[TimeLoop]
DtInitial = 1e-4 # [s]
TEnd = 1e+4 # [s]
#HaveMaxTimeStepSize = true
#MaxTimeStepSize = 1e-4

[Grid]
UpperRight = 10 1
Cells = 10 10

[Problem]
Name = test_channel_navierstokes # name passed to the output routines
InletVelocity = 1
EnableGravity = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 1e-03

[ Newton ]
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true
MaxSteps = 500
MaxRelativeShift = 1e-8
TargetSteps = 500

[Vtk]
WriteFaceData = false

[LinearSolver2]
schurGMResRestart = 100
schurResidualReduction = 1e-13
schurVerbosity = 0
schurMaxIter = 2000
precVerbosityA = 0
precVerbositySchurAppr = 0
precMaxIter = 2000
precResidReduction = 1e-13

[Algorithm]
AlgorithmType = 4
#no SIMPLEC for Stokes

[Underrelaxation]
HundredTimesVelocityUnderrelaxationFactor = 100
HundredTimesPressureUnderrelaxationFactor = 100
HundredTimesPISOSecondStepVelocityUnderrelaxationFactor = 100
HundredTimesPISOSecondStepPressureUnderrelaxationFactor = 100
