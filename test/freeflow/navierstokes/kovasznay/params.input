[Grid]
#LowerLeft = 0 0
#UpperRight = 1 1
#Cells = 20 20
File = ./grids/innercircle_quads.msh
Refinement = 0

[Problem]
Name = test_kovasznay # name passed to the output routines
EnableGravity = false
PrintL2Error = true
EnableInertiaTerms = true

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.025

[ Newton ]
MaxSteps = 10
MaxRelativeShift = 1e-5

[LinearSolver]
ResidualReduction = 1e-5
Preconditioner.Iterations = 3

[Vtk]
WriteFaceData = false

[Flux]
UpwindWeight = 0.5

[FreeFlow]
EnableUnsymmetrizedVelocityGradient = false
EnableDilatationTerm = false
