[Grid]
Positions0 = 0 6.3
Positions1 = 0 6.3
Cells0 = 50
Cells1 = 50
Grading0 = 1.0
Grading1 = 1.0

[Problem]
Name = test_sincos_steady # name passed to the output routines
EnableGravity = false
PrintL2Error = true
EnableInertiaTerms = true

[Component]
LiquidDensity = 1.0
LiquidKinematicViscosity = 0#100#0.001

[Newton]
MaxSteps = 100
MaxRelativeShift = 1e-10
SatisfyResidualAndShiftCriterion = true
EnableAbsoluteResidualCriterion = true
MaxAbsoluteResidual = 1e-10
UseLineSearch = true

[Vtk]
WriteFaceData = false
AddProcessRank = false

[Flux]
UpwindWeight = 0.5
