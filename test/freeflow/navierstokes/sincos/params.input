[TimeLoop]
DtInitial = 0.01 # [s]
TEnd = 1.0 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 6.28 6.28
Cells = 4 4

[Problem]
Name = test_ff_sincos
EnableGravity = false
PrintL2Error = true
EnableInertiaTerms = false
IsStationary = true
PrintMatrix = true

[Component]
LiquidDensity = 1.0
LiquidKinematicViscosity = 0.1

[Assembly]
NumericDifference.BaseEpsilon = 1e3

[Newton]
MinSteps = 1
EnableAbsoluteResidualCriterion = true
MaxAbsoluteResidual = 1e-7

[Vtk]
WriteFaceData = false
AddProcessRank = false

[Flux]
UpwindWeight = 0.5

[LinearSolver]
Type = restartedflexiblegmressolver
Verbosity = 1
GMResRestart = 50

[MatrixConverter]
DeletePatternEntriesBelowAbsThreshold = 1e-12

[LinearSolver.Preconditioner]
Type = uzawa
Verbosity = 1
Iterations = 5
