[TimeLoop]
DtInitial = 0.01 # [s]
TEnd = 1.0 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 6.28 6.28
Cells = 10 10

[Problem]
Name = test_ff_sincos
EnableGravity = false
PrintErrors = true
EnableInertiaTerms = true
IsStationary = true

[Component]
LiquidDensity = 1.0
LiquidKinematicViscosity = 0.1

[Assembly]
NumericDifference.BaseEpsilon = 1e-4

[Newton]
MaxSteps = 10
TargetSteps = 4
MaxRelativeShift = 1e-5
SatisfyResidualAndShiftCriterion = true
EnableAbsoluteResidualCriterion = true
MaxAbsoluteResidual = 1e-10

[Vtk]
WriteFaceData = false
AddProcessRank = false

[Flux]
UpwindWeight = 0.5

[LinearSolver]
Type = restartedflexiblegmressolver
Verbosity = 0
GMResRestart = 50

[LinearSolver.Preconditioner]
Type = uzawa
Verbosity = 1
Iterations = 5

[SpatialParams]
Temperature = 298.0

[FreeFlow]
EnableUnsymmetrizedVelocityGradient = false
EnableWeakSymmetry = true
