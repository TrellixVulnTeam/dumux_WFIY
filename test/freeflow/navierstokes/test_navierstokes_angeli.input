[TimeLoop]
DtInitial = 1e-2 # [s]
TEnd = 2e-2 # [s]
HaveMaxTimeStepSize = true
MaxTimeStepSize = 1e-2

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 10 10

[Problem]
Name = test_angeli # name passed to the output routines
EnableGravity = false
PrintL2Error = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 1

[Assembly]
NumericDifference.BaseEpsilon = 1e-8

[Newton]
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true
MaxSteps = 100
MaxRelativeShift = 1e-8
TargetSteps = 100
MaxTimeStepDivisions = 0

[Vtk]
WriteFaceData = false

[LinearSolver2]
schurGMResRestart = 100
schurResidualReduction = 1e-10
schurVerbosity = 0
schurMaxIter = 2000
precVerbosityA = 0
precVerbositySchurAppr = 0
precMaxIter = 2000
precResidReduction = 1e-13
