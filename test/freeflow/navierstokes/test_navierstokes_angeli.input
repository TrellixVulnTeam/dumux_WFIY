[TimeLoop]
DtInitial = 1e-4 # [s]
TEnd = 1e-2 # [s]
HaveMaxTimeStepSize = true
MaxTimeStepSize = 1e-4

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 40 40

[Problem]
Name = test_angeli # name passed to the output routines
EnableGravity = false
PrintL2Error = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.1

[Assembly]
NumericDifference.BaseEpsilon = 1e-8

[Newton]
MaxSteps = 100
MaxRelativeShift = 1e-12
TargetSteps = 100

[Vtk]
WriteFaceData = false

[Underrelaxation]
HundredTimesVelocityUnderrelaxationFactor = 70

[Algorithm]
AlgorithmType = 2
