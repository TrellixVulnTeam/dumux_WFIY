[TimeLoop]
DtInitial = 1e-13 # [s]
TEnd = 1e-2 # [s]

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 20 20

[Problem]
Name = test_angeli # name passed to the output routines
EnableGravity = false
PrintL2Error = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.1

[Assembly]
NumericDifference.BaseEpsilon = 1e-8

[Newton]
MaxSteps = 100
MaxRelativeShift = 1e-5
TargetSteps = 100

[Vtk]
WriteFaceData = false

[Underrelaxation]
HundredTimesVelocityUnderrelaxationFactor = 70
