[TimeLoop]
DtInitial = 1e-8 # [s]
TEnd = 1e-2 # [s]
HaveMaxTimeStepSize = true
MaxTimeStepSize = 1e-8

[Grid]
LowerLeft = 0 0
UpperRight = 1 1
Cells = 50 50

[Problem]
Name = test_angeli # name passed to the output routines
EnableGravity = false
PrintL2Error = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 0.1

[Assembly]
NumericDifference.BaseEpsilon = 1e-8

[Newton]
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true
MaxSteps = 10
MaxRelativeShift = 1e-8
TargetSteps = 10

[Vtk]
WriteFaceData = false

[Underrelaxation]
HundredTimesVelocityUnderrelaxationFactor = 100

[LinearSolver2]
schurGMResRestart = 100
schurResidualReduction = 1e-10
schurVerbosity = 0
schurMaxIter = 2000
precVerbosityA = 0
precVerbositySchurAppr = 0
precMaxIter = 2000
precResidReduction = 1e-13

[Algorithm]
AlgorithmType = 4
