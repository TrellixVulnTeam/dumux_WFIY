[Grid]
UpperRight = 1 1
Cells = 40 40

[Problem]
Name = test_donea # name passed to the output routines
EnableGravity = false
PrintL2Error = false

[ Newton ]
MaxSteps = 100
MaxRelativeShift = 1e-10

[Vtk]
WriteFaceData = false

#[LinearSolver]
#Verbosity = 2

[ InitialCondition ]
# 0: zero
# 1: exact solution
InitialConditionType = 0

[LinearSolver]
GMResRestart = 100
ResidualReduction = 1e-7
Verbosity = 0
MaxIterations = 2000
PreconditionerVerbosity = 0
SchurIterations = 1
VelocityIterations = 3
SchurResidualReduction = 1e-10
VelocityResidualReduction = 1e-10
