[Grid]
UpperRight = 1 1
Cells =5 5

[Problem]
Name = test_donea # name passed to the output routines
EnableGravity = false
PrintL2Error = false

[ Newton ]
MaxSteps = 10000
MaxRelativeShift = 1e-10
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true

[Vtk]
WriteFaceData = false

[ InitialCondition ]
# 0: zero
# 1: exact solution
InitialConditionType = 0

[ Component ]
LiquidKinematicViscosity = 1e-3

[LinearSolver2]
schurGMResRestart = 100
schurResidualReduction = 1e-10
schurVerbosity = 0
schurMaxIter = 2000
precVerbosityA = 0
precVerbositySchurAppr = 0
precMaxIter = 2000
precResidReduction = 1e-13

[Algorithm]
AlgorithmType = 2
#no SIMPLEC for Stokes

[Underrelaxation]
HundredTimesVelocityUnderrelaxationFactor = 100
HundredTimesPressureUnderrelaxationFactor = 100
