[TimeLoop]
DtInitial = 0.005 # [s]
TEnd = 0.005 # [s]
MaxTimeStepSize = 0.05

[Grid]
UpperRight = 1 1
Cells = 100 100

[Problem]
Name = flow_reduction_channel_2000 # name passed to the output routines
OutputInterval = 0.1
InletVelocity = 1.0 #1.2e-3
EnableGravity = false
EnableInertiaTerms = false
IsStationary = false
EnableTransitive = false
OutletCondition = Outflow
PrintMatrix = false
UseVelocityProfile = true
InletDelay = 0.0
InletWindup = 0.0

[Component]
LiquidDensity = 1.0 #1e3
LiquidKinematicViscosity = 1.0

GasDensity = 1
GasKinematicViscosity = 1e-6

[ Newton ]
MaxSteps = 11
MaxRelativeShift = 1e-10
UseLineSearch = false
EnableChop = false

[Vtk]
WriteFaceData = false

[Assembly]
NumericDifference.BaseEpsilon =  1e-3

# This test does actually not require setting the primary variable magnitudes.
# This rather serves as a demonstration of how to use the feature.
# [CellCenter.Assembly]
# NumericDifference.PriVarMagnitude = 1e5 # pressure is in the order of 1e5
#
# [Face.Assembly]
# NumericDifference.PriVarMagnitude = 1 # velocity is in the order of 1

[FreeFlow]
EnableDilatationTerm = false

[Phasefield]
xi = 5e-2
delta = 1e-6
sigma = 5e-3
DiffCoeff = 0.2 #1e-9
InitialConcentration = 1.0 #[mol m-3?]
InletConcentration = 1.0 #[mol m-3?]
InitialAConcentration = 2.0 #[mol m-3?]
InitialBConcentration = 1.0 #[mol m-3?]
InitialCConcentration = 1.0 #[mol m-3?]
InletAConcentration = 2.0 #[mol m-3?]
InletBConcentration = 1.0 #[mol m-3?]
InletCConcentration = 1.0 #[mol m-3?]
MineralDMolarDensity = 20.0 # 20 #[mol m-3]
MineralPMolarDensity = 4.0 # 4 #[mol m-3]
Scenario = Grain # ThinChannel or Grain
GrainX = 0.5
GrainY = 0.5
StartingRadius = 0.2
StartingScaling = 10.0
StartingScaling2 = 3.8
DirichletRadius = 0.2
DirichletScaling = 9.0
K = 2000.0 #25.0
n = 10.0
Reaction = 1.0 #8.9e-3
AdvectiveTransport = 1.0

[Restart]
#Time = 1.11325
#File = compare_flow_1s25_10000K_gammapos
