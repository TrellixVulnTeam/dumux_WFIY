[TimeLoop]
DtInitial = 1 # [s]
TEnd = 2 # [s]

[Grid]
UpperRight = 5 2
Cells = 50 40

[Problem]
Name = test_channel_navier_stokes_new # name passed to the output routines
InletVelocity = 30
EnableGravity = false
EnableInertiaTerms = true
OutletCondition = Outflow
PrintMatrix = false
UseVelocityProfile = true

[Component]
LiquidDensity = 1.0
LiquidKinematicViscosity = 1.0

GasDensity = 1
GasKinematicViscosity = 1e-6

[ Newton ]
MaxSteps = 10
MaxRelativeShift = 1e-8

[Vtk]
WriteFaceData = false

[Assembly]
NumericDifference.BaseEpsilon =  1e-3

# This test does actually not require setting the primary variable magnitudes.
# This rather serves as a demonstration of how to use the feature.
# [CellCenter.Assembly]
# NumericDifference.PriVarMagnitude = 1e5 # pressure is in the order of 1e5
#
# [Face.Assembly]
# NumericDifference.PriVarMagnitude = 1 # velocity is in the order of 1

[FreeFlow]
EnableDilatationTerm = false

[Phasefield]
xi = 0.25
delta = 1e-8
sigma = 3.75e-3
DiffCoeff = 0.2
InitialConcentration = 1.0
StartingRadius = 0.3
StartingScaling = 3.0
DirichletRadius = 0.5
DirichletScaling = 3.0
K = 100.0
n = 10.0
Reaction = 0.0
AdvectiveTransport = 0.0
