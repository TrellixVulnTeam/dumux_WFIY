[Grid]
UpperRight = 1 1
Cells = 10 10
Refinement = 2
#File = ./grids/2dRandCubic_0.dgf
#Refinement = 3

[Problem]
Name = test_donea # name passed to the output routines
EnableGravity = false
PrintL2Error = true
EnableInertiaTerms = false
PrintMatrix = false

[Component]
LiquidDensity  = 1.0
LiquidKinematicViscosity = 1.0

[ Newton ]
MaxSteps = 10
MaxRelativeShift = 1e-5

[Vtk]
WriteFaceData = false

[FreeFlow]
EnableUnsymmetrizedVelocityGradient = false

[Assembly]
NumericDifference.BaseEpsilon = 1e-8
