[Grid]
UpperRight = 1 1
Cells = 8 8
Overlap = 1

[Problem]
Name = test_donea # name passed to the output routines
EnableGravity = false
PrintL2Error = false
EnableInertiaTerms = false

[Component]
LiquidDensity  = 1.0
LiquidKinematicViscosity = 1.0

[ Newton ]
MaxSteps = 10
MaxRelativeShift = 1e-3

[Vtk]
WriteFaceData = false

[LinearSolver]
Type = bicgstabsolver
Verbosity = 2

[LinearSolver.Preconditioner]
Type = ssor
