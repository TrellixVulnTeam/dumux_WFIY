[Grid]
UpperRight = 1 1
Cells = 8 8
Overlap = 1

[Problem]
Name = test_donea # name passed to the output routines
EnableGravity = false
PrintL2Error = false
EnableInertiaTerms = false
PrintMatrix = false

[Component]
LiquidDensity  = 1.0
LiquidKinematicViscosity = 1.0

[ Newton ]
MaxSteps = 30
MaxRelativeShift = 1e-8
MinSteps = 2

[Vtk]
WriteFaceData = false

[LinearSolver]
Type = bicgstabsolver
# Type = restartedgmressolver
Verbosity = 1
ResidualReduction = 1e-13

[LinearSolver.Preconditioner]
Type = amg
AmgAccumulationMode = atOnce
AmgMaxLevel = 2
Verbosity = 2


[Assembly]
NumericDifference.BaseEpsilon = 1e-3

# [Assembly]
# NumericDifferenceMethod = 0

[FreeFlow]
EnableUnsymmetrizedVelocityGradient = true
