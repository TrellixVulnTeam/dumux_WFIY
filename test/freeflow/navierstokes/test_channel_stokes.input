[TimeLoop]
DtInitial = 1 # [s]
TEnd = 2 # [s]

[Grid]
UpperRight = 10 1
Cells = 100 50

[Problem]
Name = test_channel_stokes # name passed to the output routines
InletVelocity = 1
EnableGravity = false

[ Newton ]
MaxSteps = 10
MaxRelativeShift = 1e-8

[Vtk]
AddVelocity = true
WriteFaceData = false


[Assembly]
NumericDifference.BaseEpsilon =  1e-4
NumericDifference.PriVarMagnitude =  1e5 1 1
