[TimeLoop]
DtInitial = 1 # [s]
TEnd = 2 # [s]
HaveMaxTimeStepSize = true
MaxTimeStepSize = 1

[Grid]
UpperRight = 1 1
Cells = 10 10

[Problem]
Name = test_liddrivencavity_re1 # name passed to the output routines
LidVelocity = 1
EnableGravity = false

[Component]
LiquidDensity = 1
LiquidKinematicViscosity = 1e+3

[ Newton ]
EnableShiftCriterion = false
EnableSIMPLEAbsoluteResidualCriterion = true
MaxSteps = 10000
MaxRelativeShift = 1e-9

[Vtk]
WriteFaceData = false

[LinearSolver2]
schurGMResRestart = 100
schurResidualReduction = 1e-10
schurVerbosity = 0
schurMaxIter = 2000
precVerbosityA = 0
precVerbositySchurAppr = 0
precMaxIter = 2000
precResidReduction = 1e-13

[Algorithm]
AlgorithmType = 3


[Underrelaxation]
HundredTimesVelocityUnderrelaxationFactor = 1
HundredTimesPressureUnderrelaxationFactor = 1
