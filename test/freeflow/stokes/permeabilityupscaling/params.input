[Grid]
#BinaryMask = ATA_Sample1_crop_small.raw
#Cells = 100 100 100
BinaryMask = sphere.raw
Cells = 13 11 15
PixelDimensions = 2 2 2

[Problem]
Name = test_stokes_upscaling
EnableGravity = false
EnableInertiaTerms = false
Alpha = 0.0
Beta = 1.0
#DomainLength = 1e-3
DomainLength = 26e-6

[Component]
# this test requires a unit fluid
LiquidDensity = 1
LiquidDynamicViscosity = 1

[Assembly.NumericDifference]
PriVarMagnitude = 1
BaseEpsilon = 0.1

[LinearSolver]
Verbosity = 2
MaxIterations = 2000
ResidualReduction = 1e-8

[LinearSolver.Preconditioner]
Verbosity = 2
Iterations = 1
AmgSmootherIterations = 1
AmgCriterionSymmetric = true
AmgMaxLevel = 10
AmgDefaultAggregationDimension = 3
AmgMinAggregateSize = 3
AmgMaxAggregateSize = 3

[Newton]
MinSteps = 1
EnableAbsoluteResidualCriterion = true
MaxAbsoluteResidual = 1e3

[Vtk]
WriteFaceData = false

[FluxOverPlane]
Verbose = true
