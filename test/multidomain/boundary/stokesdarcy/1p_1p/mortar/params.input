[Darcy.Grid]
LowerLeft = 0 0 0
UpperRight = 1 1 1
Cells = 5 5 2

[Darcy2.Grid]
LowerLeft = 0 0 1
UpperRight = 1 1 2
Cells = 5 5 2

[Mortar.Grid]
LowerLeft = 0 0 1
UpperRight = 1 1 1
Cells = 5 5

[Mortar]
ProjectionIntegrationOrder = 1

[InterfaceSolver]
Verbosity = 3
ResidualReduction = 1e-8
MaxIterations = 1000

[Darcy.Problem]
Name = darcy

[Darcy2.Problem]
Name = darcy2

[Darcy.SpatialParams]
Permeability = 1.0 # m^2
AlphaBeaversJoseph = 1.0

[Darcy2.SpatialParams]
Permeability = 1.0 # m^2
AlphaBeaversJoseph = 1.0

[Vtk]
AddVelocity = 1
OutputName = stokesdarcy

[Problem]
EnableGravity = false
EnableInertiaTerms = false
TopBoundaryPressure = 0.0
BottomBoundaryPressure = 1.0
TopSource = 0.0

[0.Component]
LiquidDensity = 1
LiquidKinematicViscosity = 1
