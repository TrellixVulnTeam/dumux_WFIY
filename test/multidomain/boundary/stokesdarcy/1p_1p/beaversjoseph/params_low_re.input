[TimeLoop]
TEnd = 1e5
DtInitial = 100

[Grid]
Positions0 = 0 0.75
Positions1 = 0 0.25
Cells0 = 150
Cells1 = 50

[Darcy.Grid]
File = ./unstructured.msh # to be used for unstructured test case
LowerLeftPM = 0.25 0
UpperRightPM = 0.5 0.20

[Stokes.Grid]
LowerLeft = -0.5 0
UpperRight = 1.5 0.5
Cells = 400 50

[Stokes.Problem]
Name = stokes
PressureDifference = 1e-6
PressureGradientForBJOutput = 8.278e-10

[Darcy.Problem]
Name = darcy

[Darcy.SpatialParams]
K1 = 1e-6
K2 = 1e-6
PermeabilityAngle = 45
AlphaBeaversJoseph = 1.0

[Problem]
Name = test_stokes1pdarcy1phorizontal
Outputname = mpfa_lowre
EnableGravity = false
EnableInertiaTerms = true

[Vtk]
AddVelocity = 1
WriteFaceData = true

[Darcy.Vtk]
AddVelocity = 1

[Mpfa]
Q = 0.5

[Newton]
MaxRelativeShift = 1e-7

[Assembly]
NumericDifferenceMethod = 0
NumericDifference.BaseEpsilon = 1e-8

[FluxOverSurface]
Verbose = false
PosX = 0.375
