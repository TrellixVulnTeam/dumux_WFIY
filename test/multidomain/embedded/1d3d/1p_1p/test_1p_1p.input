[MixedDimension]
NumCircleSegments = 100
IntegrationOrder = 3

[TimeLoop]
DtInitial = 1 # [s]
TEnd = 1 # [s]
MaxTimeStepSize = 1

[Vessel.Grid]
LowerLeft = 0 0 0
UpperRight = 0 0 1
Cells = 40

[Tissue.Grid]
#File = ./grids/cylinder.msh
#Refinement = 10
LowerLeft = -1 -1 0
UpperRight = 1 1 1
Cells = 40 40 40

[Problem]
Name = test_1p1p_embedded
NormIntegrationOrder = 2
NormExcludeInnerBulk = true
EnableGravity = false

[Component]
LiquidKinematicViscosity = 1 # [m^2/s]
LiquidDensity = 1 # [kg/m^3]

[SpatialParams]
PermeabilityTissue = 1 # [m^2]
Radius = 0.3 # [m]

[Vtk]
AddVelocity = false

[BoundaryConditions1D]
PressureInput = 2.0
DeltaPressure = 1.0

[Newton]
MaxRelativeShift = 1e-7
MaxSteps = 10

[LinearSolver]
ResidualReduction = 1e-20
