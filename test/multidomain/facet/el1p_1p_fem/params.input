[Problem]
EnableGravity = false
ExtractionPressure = 0
InjectionOverPressure = 1e5
UseInteriorDirichlet = true

[Component]
SolidDensity = 2750.0 # granite

[Grid]
File = ./grids/singlefracture.msh

[SpatialParams]
InitialPorosity = 0.15 # used from both ElasticBulk & OnePBulk
InitialGap = 1e-4
MinHydraulicAperture = 1e-7
ZeroGapThreshold = 1e-15

[OnePBulk]
Problem.Name = onep_bulk
Problem.BoxUseDirichletOutlet = false
SpatialParams.InitialPermeability = 1e-14
SpatialParams.UseConstantPorosity = true

[OnePFacet]
Problem.Name = onep_facet

[Lagrange]
Grid.File = ./grids/singlefracture_lagrange.msh
Problem.Name = lagrange
InitialTraction = 0.0 0.0
PenaltyFactor = 1e8
FrictionCoefficient = 0.5
Assembly.FEIntegrationOrder = 2

[ElasticBulk]
Problem.Name = elastic_bulk
SpatialParams.EModule = 1e9
SpatialParams.Nu = 0.25
SpatialParams.BiotCoefficient = 1.0
Assembly.FEIntegrationOrder = 2
Assembly.FEBoundaryIntegrationOrder = 2

[ContactProblem]
ContactForceIntegrationOrder = 2

[Newton]
MaxSteps = 40
SatisfyResidualAndShiftCriterion = false
EnableAbsoluteResidualCriterion = true
EnableResidualCriterion = false
EnableShiftCriterion = true
ResidualReduction = 1e-20
MaxAbsoluteResidual = 5.0e-6
MaxRelativeShift = 1e-8
UseLineSearch = false

[Vtk]
OutputName = densenetwork
AddVelocity = true

[Output]
FluxFile = fluxes.csv

[0.Component]
LiquidKinematicViscosity = 1e-6
LiquidDensity = 1000
