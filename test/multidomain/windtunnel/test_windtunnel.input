[TimeManager]
InitTime = 0 # [s] Initialization time without coupling
DtInitial = 1e-3 # [s]
MaxTimeStepSize = 3600 # [s]
EpisodeLength = 10800 # [s]
TEnd = 864000 # [s]
AbortIfEvapRateIsConstant = true

[Grid]
Cells0 = 32
Cells1 = 32 32
Positions0 = 0.0 0.25
Positions1 = 0.0 0.25 0.5
Grading1 = -1.18574 1.18574

NoDarcyX1 = 0.0  # [m] # Beginning of PM below
NoDarcyX2 = 0.25 # [m] # End of PM below
InterfacePosY = 0.25 # [m] # Vertical position of coupling interface
Verbosity = true

[Output]
PlotMaterialLaw = false
PlotEvaporationRate = false
FreqFluxOutput = 10
Name = parallel


###############################################################
# Define conditions in the two subdomains
###############################################################
[FreeFlow]
RefVelocity = 3.5 # [m/s]
RefPressure = 1e5 # [Pa]
RefMassFraction = 0.008 # [-]
RefTemperature = 298.15 # [K]

[Coupling]
Method = 1

[Problem]
UseSmallFluidSystemTable = false

[PorousMedium]
InitialPhasePresence = "bothPhases" # bothPhases, nPhaseOnly
SolDependentEnergyWalls = false
SolDependentEnergySource = false
PlexiglassThermalConductivity = 1e9 # 0.116 # [W/(m*K)]
PlexiglassThickness = 0.0125 # [m]
RefPressure = 1e5 # [Pa]
RefTemperature = 298.15 # [K] # mean from first 5 data rows
RefWaterSaturation = 0.98 # [-]

[SpatialParams]
NumberOfSoilLayers = 1
SoilLayerIdx1 = 1
SoilLayerIdx2 = 1

[Soil1]
# Mines
AlphaBJ = 1.0 # [-]
Permeability = 2.65e-10 # [m^2]
Porosity = 0.41 # [-]
Swr = 0.005 # [-
Snr = 0.01 # [-]
VgAlpha = 6.371e-4 # [1/Pa]
VgN = 8.0 # [-]
ThermalConductivitySolid = 5.26 # [W/(m*K)]


###############################################################
# Newton and Linear solver parameters
###############################################################
[Newton]
MaxRelativeShift = 1e-4
ResidualReduction = 1e-7
TargetSteps = 5
MaxSteps = 9
TimeStepReduction = 0.5


###############################################################
# Eddy Viscosity Models
# 0 = none
# 1 = Prandtl
# 2 = modified Van Driest
# 3 = Baldwin Lomax
###############################################################
# Eddy Diffusivity and Eddy Conductivity Models
# 0 = none
# 1 = Reynolds analogy
# 2 = modified Van Driest
# 3 = Deissler
# 4 = Meier and Rotta
###############################################################
[ZeroEq]
EddyViscosityModel = 0 # 2
EddyDiffusivityModel = 0 # 1
EddyConductivityModel = 0 # 1
