[TimeManager]
DtInitial = 1 # [s]
TEnd = 1 # [s]
MaxTimeStepSize = 1
DtInitialBulkProblem = 1 # [s]
DtInitialLowDimProblem = 1 # [s]
EpisodeTime = 1 # [s]

[Grid]
File = ./grids/singlefracturequadrilateral.msh
NumCells = 10

[Problem]
Name = test_1dfracture

[MixedDimension]
UseIterativeSolver = False

[IterativeAlgorithm]
MaxIterations = 100
Tolerance = 1.0e-8
Verbose = 1

[SpatialParams]
FractureAperture = 1e-3 # [m]
FracturePermeability = 1e4 # [m^2]
MatrixPermeability = 1 # [m^2]

[Vtk]
AddVelocity = False

[Newton]
MaxRelativeShift = 1e-5
MaxSteps = 20

[L2Error]
IntegrateElements = false
QuadratureOrder = 2
