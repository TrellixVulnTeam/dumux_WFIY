[TimeManager]
DtInitial = 1e-2 # [s]
TEnd = 3153600000 # [s]
DtInitialBulkProblem = 1 # [s]
DtInitialLowDimProblem = 1 # [s]
EpisodeTime = 3153600000 # [s]

[Grid]
File = ./grids/real_modified.msh

[Problem]
Name = test_real_ni

[MixedDimension]
UseIterativeSolver = False

[IterativeAlgorithm]
MaxIterations = 100
Tolerance = 1.0e-8
Verbose = 1

[SpatialParams]
FractureAperture = 2e-4 # [m]
FracturePermeability = 5e-11 # [m^2]
MatrixPermeability = 1e-13 # [m^2]

[Mpfa]
Q = 0.33
Xi = 0.66

[Vtk]
AddVelocity = False

[Newton]
MaxRelativeShift = 1e-5
MaxSteps = 20
