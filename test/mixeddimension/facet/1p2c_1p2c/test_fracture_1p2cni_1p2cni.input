[TimeManager]
DtInitial = 1e-2 # [s]
TEnd = 1 # [s]
DtInitialBulkProblem = 1 # [s]
DtInitialLowDimProblem = 1 # [s]
EpisodeTime = 1 # [s]

[Grid]
File = ./grids/durlovsk.msh

[Problem]
Name = test_1dfracture_ni

[MixedDimension]
UseIterativeSolver = False

[IterativeAlgorithm]
MaxIterations = 100
Tolerance = 1.0e-8
Verbose = 1

[SpatialParams]
FractureAperture = 1e-5 # [m]
FracturePermeability = 1e-7 # [m^2]
MatrixPermeability = 1e-12 # [m^2]

[Vtk]
AddVelocity = False

[Newton]
MaxRelativeShift = 1e-5
MaxSteps = 20
