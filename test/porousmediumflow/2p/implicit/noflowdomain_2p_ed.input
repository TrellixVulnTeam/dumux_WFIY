[TimeManager]
DtInitial = 1 # [s]
MaxTimeStepSize = 250 # [s]
TEnd = 40000 # [s]

[Grid]
LowerLeft = -0.5 -0.5
UpperRight = 0.5 0.5
Cells = 200 200

[Problem]
Name = noflowdomain_2p # name passed to the output routines
EnableGravity = 0 # disable gravity
OverPressure = 1e5
TimeForFractureEdgePlots = 10000

[SpatialParams]
MatrixPermeability = 1e-13
FracturePermeability = 1e-9
MatrixPorosity = 0.2
FracturePorosity = 0.8
FractureAperture = 1e-2
MatrixPe = 1e4
MatrixLambda = 2
FracturePe = 1e3
FractureLambda = 2

[Mpfa]
Q = 0.0

[Vtk]
AddVelocity = 0 # enable velocity output
