[TimeLoop]
DtInitial = 1 # [s]
TEnd = 1.0e6 #1.0e5 # [s]
MaxTimeStepSize= 4000 # Set the maximum time step size

[Grid]
UpperRight = 1 1
######## for layered and homogeneous systems:
#Cells = 1 50
######## for heterogeneous lenses:
Cells = 20 20

[Problem]
Name = GSRDiffusionTest # name passed to the output routines
EnableGravity = false
######## Values to change ########
Temperature = 327.15 # [K] standard: 296.15 K
InitialAirSaturation = 0.5
HasInitialXylenePhase = false #true #false
InitialXylene = 0.001 #1.0e-9 #depending on "HasInitialXylenePhase" either the xylene phase saturation or the mole fraction in air

[Assembly]
NumericDifferenceMethod = 0 # -1 backward differences, 0: central differences, +1: forward differences

[Component]
SolidDensity = 2650
SolidThermalConductivity = 2.8
SolidHeatCapacityFine = 850
SolidHeatCapacityCoarse = 84000

[SpatialParams]
Swr = 0.12
Snr = 0.10
Sgr = 0.01
ParkerVanGenuchtenN = 4
ParkerVanGenuchtenRegardSnrForKrn = true
######## Values to change ########
HasLayers = false #true #false #have either layers or heterogeneous lenses
IsHeterogen = true #false
Fine.ParkerVanGenuchtenAlpha = 0.05 # 0.05 # set to same value for "only porosity effect" do not change more than 1 order of magnitude for a fast simulation.
Coarse.ParkerVanGenuchtenAlpha = 0.5
FineLayerThickness = 0.2 # for layers: Layer thickness, for (square) lenses: lense width & height
Fine.Permeability = 1.0e-8 # 1.0e-15  # m^2
Coarse.Permeability = 1.0e-8  # m^2
Fine.Porosity = 0.3
Coarse.Porosity = 0.5
UpperFine = 0.8 # for layers: height (z-coord.) of the upper layer center; for lenses: x and z coordinate of the (square) lense center
LowerFine = 0.3 # for layers: height (z-coord.) of the lower layer center; for lenses: x and z coordinate of the (square) lense center